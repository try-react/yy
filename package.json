{
  "name": "4",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "// CI向けのスクリプト": "================================================================================",
    "madge:domain": "yarn madge --image docs/madge/domain/graph.svg src/domain",
    "madge:useCase": "yarn madge --image docs/madge/useCase/graph.svg src/useCase",
    "madge:externalInterface": "yarn madge --image docs/madge/externalInterface/graph.svg src/externalInterface",
    "madge:pages": "yarn madge --image docs/madge/pages/graph.svg pages",
    "madge:all": "yarn madge --image docs/madge/graph.svg src",
    "madge:gateway": "yarn madge --image docs/madge/gateway/graph.svg src/gateway",
    "madge:presenter": "yarn madge --image docs/madge/presenter/graph.svg src/presenter",
    "valid:type": "yarn tsc --noEmit",
    "valid:lint": "yarn eslint --fix '{src,pages}/**/*.{ts,tsx}'",
    "valid:test": "yarn jest",
    "valid:jscpd": "yarn jscpd src",
    "// CI": "================================================================================",
    "prelocal-ci": "circleci config process .circleci/config.yml > process.yml",
    "local-ci": "circleci local execute -c process.yml"
  },
  "dependencies": {
    "@cybozu/eslint-config": "10.0.4",
    "@testing-library/react": "10.2.1",
    "@testing-library/react-hooks": "3.3.0",
    "@types/jest": "26.0.0",
    "babel-plugin-react-remove-properties": "0.3.0",
    "commitizen": "4.1.2",
    "cz-emoji": "1.2.1",
    "eslint-plugin-jest": "23.13.2",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.9",
    "netlify-cli": "2.53.0",
    "next": "9.4.4",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-content-loader": "5.0.4",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "type-fest": "0.15.1",
    "typescript": "3.9.5"
  },
  "devDependencies": {
    "@types/node": "14.0.13",
    "@types/react": "16.9.35",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "eslint": "7.2.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "jscpd": "3.3.0-rc.8",
    "madge": "3.9.1",
    "stylelint": "13.6.0",
    "stylelint-config-standard": "20.0.0",
    "typescript-plugin-css-modules": "2.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    },
    "cz-emoji": {
      "skipQuestions": [
        "scope"
      ]
    }
  }
}

{
  "name": "@app/front",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "node server.js",
    "// CI向けのスクリプト": "================================================================================",
    "build": "next build && next export",
    "// madge": "",
    "madge:domain": "yarn madge --image docs/madge/domain/graph.svg src/domain",
    "madge:useCase": "yarn madge --image docs/madge/useCase/graph.svg src/useCase",
    "madge:externalInterface": "yarn madge --image docs/madge/externalInterface/graph.svg src/externalInterface",
    "madge:pages": "yarn madge --image docs/madge/pages/graph.svg pages",
    "madge:all": "yarn madge --image docs/madge/graph.svg src",
    "madge:gateway": "yarn madge --image docs/madge/gateway/graph.svg src/gateway",
    "madge:presenter": "yarn madge --image docs/madge/presenter/graph.svg src/presenter",
    "// valid": "",
    "valid:type": "yarn tsc --noEmit",
    "valid:lint": "yarn eslint --fix '{src,pages}/**/*.{ts,tsx}'",
    "valid:test": "yarn jest",
    "valid:jscpd": "yarn jscpd ./src"
  },
  "dependencies": {
    "@material-ui/core": "4.12.1",
    "@material-ui/icons": "4.11.2",
    "lint-staged": "10.5.4",
    "madge": "3.9.2",
    "netlify-cli": "3.8.5",
    "next": "10.2.2",
    "react": "17.0.2",
    "react-content-loader": "6.0.3",
    "react-dom": "17.0.2",
    "type-fest": "1.2.1",
    "typescript": "4.0.3",
    "wretch": "1.7.4"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "11.0.1",
    "@testing-library/react": "11.2.2",
    "@testing-library/react-hooks": "5.1.2",
    "@types/jest": "26.0.23",
    "@types/node": "15.3.0",
    "@types/react": "16.9.49",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.15.2",
    "babel-plugin-react-remove-properties": "0.3.0",
    "eslint": "7.18.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "26.6.3",
    "prettier": "2.0.5",
    "react-content-loader": "6.0.3",
    "stylelint": "13.6.1",
    "stylelint-config-standard": "20.0.0",
    "typescript-plugin-css-modules": "2.4.0"
  }
